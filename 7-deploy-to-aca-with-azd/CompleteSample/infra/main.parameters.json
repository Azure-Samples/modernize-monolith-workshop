    "contentVersion": "1.0.0.0",
    "parameters": {
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "pg_products_password": {
        "value": "${AZURE_PG_PRODUCTS_PASSWORD}"
      },
      "pg_storeinfo_password": {
